<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:Protobuf.Decode.Desktop"
             xmlns:shared="clr-namespace:Protobuf.Decode.Shared;assembly=Protobuf.Decode.Shared"
             x:Class="Protobuf.Decode.Desktop.MainPage"
             BackgroundColor="Transparent">

    <BlazorWebView x:Name="blazorWebView" HostPage="wwwroot/index.html">
        <BlazorWebView.RootComponents>
            <RootComponent Selector="#app" ComponentType="{x:Type shared:Routes}" />
        </BlazorWebView.RootComponents>
    </BlazorWebView>

</ContentPage>
